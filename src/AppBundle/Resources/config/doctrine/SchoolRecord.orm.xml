<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SchoolRecord" table="school_record">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="schoolName" type="string" column="school_name" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phoneNumber" type="string" column="phone_number" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="district" type="string" column="district" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gramaNiladhari" type="string" column="grama_niladhari" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="divisionalSecretariat" type="string" column="divisional_secretariat" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="haveElectricity" type="boolean" column="have_electricity" nullable="false"/>
    <field name="haveLab" type="boolean" column="have_lab" nullable="false"/>
    <field name="numberOfTeacher" type="integer" column="number_of_teacher" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numberOfComputer" type="integer" column="number_of_computer" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numberOfWorking" type="integer" column="number_of_working" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lackComputerParts" type="text" column="lack_computer_parts" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noOfStudents" type="integer" column="no_of_students" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="langitude" type="float" column="langitude" precision="10" scale="0" nullable="false"/>
    <field name="latitude" type="float" column="latitude" precision="10" scale="0" nullable="false"/>
    <field name="proposals" type="text" column="proposals" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="providedBy" type="string" column="provided_by" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
